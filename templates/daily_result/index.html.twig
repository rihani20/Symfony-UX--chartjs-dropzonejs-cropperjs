{% extends 'base.html.twig' %}

{% block title %}DailyResult index{% endblock %}

{% block body %}
    <h1>DailyResult index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Date</th>
                <th>Value</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for daily_result in daily_results %}
            <tr>
                <td>{{ daily_result.id }}</td>
                <td>{{ daily_result.date ? daily_result.date|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ daily_result.value }}</td>
                <td>
                    <a href="{{ path('app_daily_result_show', {'id': daily_result.id}) }}">show</a>
                    <a href="{{ path('app_daily_result_edit', {'id': daily_result.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_daily_result_new') }}">Create new</a>
{% endblock %}
